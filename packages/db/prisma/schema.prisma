generator client {
    provider   = "prisma-client-js"
    engineType = "binary"
}

datasource db {
    provider = "postgresql"
}

model User {
    id           Int      @id @default(autoincrement())
    email        String   @unique
    passwordHash String
    refreshToken String?
    theme        String   @default("system")
    createdAt    DateTime @default(now())
}
