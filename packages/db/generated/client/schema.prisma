generator client {
  provider   = "prisma-client-js"
  engineType = "binary"
  output     = "../generated/client"
}

datasource db {
  provider = "postgresql"
}

model User {
  id           Int      @id @default(autoincrement())
  email        String   @unique
  passwordHash String
  refreshToken String?
  createdAt    DateTime @default(now())
}
